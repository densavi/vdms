{{#extend layout title="admin" active="admin" path="../" animation="true" isBreadCrumb="true" }}
{{#content "content"}}


<form class="card mb-3 border-left">
  <div class="card-body">
    <h5 class="border-bottom pb-1 mb-2">Case Search</h5>

    <div class="row mb-2">
      <div class="col-12 col-lg-4 mb-2">
        <label class="form-label mb-0"><small>Select Date Range</small></label>

        <div class="ds-dates d-flex align-items-center justify-content-between gap-2">
          <div class="ds-dates__col">
            <div class="position-relative"
              onclick="document.getElementById('exampleInputdate').showPicker()">
              <input type="date" class="form-control form-control-sm" id="exampleInputdate">
            </div>
            {{!-- <input type="date" class="form-control form-control-sm" id="exampleInputdate"> --}}
          </div>
          <div class="ds-date__text">To</div>
          <div class="ds-dates__col">
            <div class="position-relative"
              onclick="document.getElementById('exampleInputdate1').showPicker()">
              <input type="date" class="form-control form-control-sm" id="exampleInputdate1">
            </div>
          </div>
        </div>

      </div>
      <div class="col-12 col-lg-4 mb-2">
        <label class="form-label mb-0" for="select-bank"><small>Select Bank/Company</small></label>
        <select id="select-bank" class="form-select form-select-sm" aria-label=".form-select-lg example">
          <option selected="">All Banks/Companies</option>
          <option>1</option>
          <option>2</option>
          <option>3</option>
        </select>
      </div>
      <div class="col-12 col-lg-4 mb-2">
        <label class="form-label mb-0" for="select-agency"><small>Select Agency</small></label>
        <select id="select-agency" class="form-select form-select-sm" aria-label=".form-select-lg example">
          <option selected="">All Firing Agencies</option>
          <option>1</option>
          <option>2</option>
          <option>3</option>
        </select>
      </div>
      <div class="col-12 col-lg-4 mb-2">
        <label class="form-label mb-0" for="select-office"><small>Select Office</small></label>
        <select id="select-office" class="form-select form-select-sm" aria-label=".form-select-lg example">
          <option selected="">All Billing Offices</option>
          <option>1</option>
          <option>2</option>
          <option>3</option>
        </select>
      </div>
      <div class="col-12 col-lg-4 mb-2">
        <label class="form-label mb-0" for="select-search-criteria"><small>Select Search Criteria</small></label>
        <select id="select-search-criteria" class="form-select form-select-sm" aria-label=".form-select-lg example">
          <option selected="">Select Search Criteria</option>
          <option>1</option>
          <option>2</option>
          <option>3</option>
        </select>
      </div>
      <div class="col-12 col-lg-4 mb-2">
        <label class="form-label mb-0" for="case-status"><small>Case Status <span
              class="text-danger">*</span></small></label>
        <select id="case-status" class="form-select form-select-sm" aria-label=".form-select-lg example" required>
          <option selected="">All</option>
          <option>1</option>
          <option>2</option>
          <option>3</option>
        </select>
      </div>
    </div>
    <div class="row mb-2">

      <div class="col-12 col-lg-4 mb-2">
        <label class="form-label mb-0" for="case-process"><small>Case Process <span
              class="text-danger">*</span></small></label>
        <select id="case-process" class="form-select form-select-sm" aria-label=".form-select-lg example" required>
          <option selected="">All</option>
          <option>1</option>
          <option>2</option>
          <option>3</option>
        </select>
      </div>
      <div class="col-12 col-lg-4 mb-2">
        <label class="form-label mb-0" for="select-team"><small>Select Team <span
              class="text-danger">*</span></small></label>
        <select id="select-team" class="form-select form-select-sm" aria-label=".form-select-lg example" required>
          <option selected="">All</option>
          <option>1</option>
          <option>2</option>
          <option>3</option>
        </select>
      </div>
      <div class="col-12 col-lg-4 mb-3 mb-lg-3">
        <label class="form-label mb-0" for="bill-type"><small>Bill Type <span
              class="text-danger">*</span></small></label>
        <select id="bill-type" class="form-select form-select-sm" aria-label=".form-select-lg example" required>
          <option selected="">All</option>
          <option>1</option>
          <option>2</option>
          <option>3</option>
        </select>
      </div>
    </div>

    <div class="row align-items-center">
      <div class="col-12 ms-auto d-flex justify-content-center gap-2">
        <button class="btn">Reset</button>
        <button class="btn btn-primary">Search</button>
      </div>
    </div>

  </div>
</form>


<div class="row">
  <div class="col-sm-12">
    <div class="card-header d-flex justify-content-between flex-wrap mb-2 align-items-center">
      <h4 class="card-title mb-0">Case - Details</h4>
      <button class="btn btn-success">Export To Excel</button>
    </div>
  </div>


  <div class="col-12">
    <div class="card">
